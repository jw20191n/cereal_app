<h1>Create Cereal</h1>
<%= form_for @cereal do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :amount %>
<%= f.text_field :amount %>
<%= f.label :img_url %>
<%= f.text_field :img_url %><br>
<%= @doc %>
<%= f.label :user_id, "Name" %>
<%= f.collection_select :user_id, User.all, :id, :name %>
<%= f.submit %>
<% end %>